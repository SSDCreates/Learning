body
    height 100%
    width 100%
    .wrapper
        display grid
        grid-template-columns repeat(6 1fr)
        grid-template-rows repeat(6 1fr)
        color red
        position absolute
        left 10%
        right 10%
        .earnings
            text-align center
            grid-row 1 / 3
            color blue
            grid-column 1 / 4 

